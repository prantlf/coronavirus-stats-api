  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "world",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bin/cov-world"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "countries",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bin/cov-countries"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "update",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bin/cov-update"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "serve",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bin/cov-serve"
    }
  ]
}