{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "world",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bin/cov-world",
      "env": {
        "DATABASE_TYPE": "sqlite"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "countries",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bin/cov-countries"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "update",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bin/cov-update"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "clear",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bin/cov-clear",
      "env": {
        "DATABASE_TYPE": "mysqlx"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "import",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bin/cov-import",
      "args": [
        "data/data.json"
      ],
      "env": {
        "DATABASE_TYPE": "mysqlx"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "serve",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bin/cov-serve"
    }
  ]
}